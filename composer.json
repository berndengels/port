{
    "name": "berndengels/port",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-ide-helper": "^2.13",
        "berndengels/laravel-email-exceptions": "dev-master",
        "fruitcake/laravel-cors": "dev-develop",
        "graham-campbell/markdown": "^15.0",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "kyslik/column-sortable": "^6.5",
        "laravel/framework": "^10.13",
        "laravel/pail": "^1.2",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "laravel/vonage-notification-channel": "^3.2",
        "laraveldaily/laravel-invoices": "^3.0",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.3",
        "mobiledetect/mobiledetectlib": "^2.8",
        "phpoffice/phpspreadsheet": "^1.29",
        "protonemedia/laravel-form-components": "^3.8",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/db-dumper": "^3.3",
        "spatie/laravel-db-snapshots": "^2.5",
        "spatie/laravel-google-fonts": "^1.2",
        "spatie/laravel-medialibrary": "^10.9",
        "spatie/laravel-permission": "^5.10",
        "spatie/laravel-webhook-client": "^3.4",
        "spatie/period": "^2.4",
        "spatie/temporary-directory": "^2.1",
        "symfony/browser-kit": "^6.3",
        "symfony/dom-crawler": "^6.3",
        "symfony/mailer": "^6.3",
        "umulmrum/holiday": "^0.6.0",
        "vonage/client": "^4.0",
        "vonage/vonage-laravel": "^1.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.2",
        "fakerphp/faker": "^1.23",
        "helmich/phpunit-json-assert": "^3.5",
        "kitloong/laravel-migrations-generator": "^6.10",
        "laravel/dusk": "^7.7",
        "laravel/envoy": "^2.8",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.6",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2"
    },
    "allow-plugins": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Database\\Data\\": "database/data/",
            "App\\Libs\\Vendor\\": "app/Libs/vendor"
        },
        "classmap": [
            "database",
            "database/seeders",
            "database/factories",
            "database/data"
        ],
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "platform-check": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
